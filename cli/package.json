  "author": "Canopy Charts Team",
  "license": "MIT",
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/inquirer": "^9.0.7",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cosmiconfig": "^8.3.6",
    "diff": "^5.1.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.3.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/diff": "^5.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
