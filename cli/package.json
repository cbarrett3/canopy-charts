{
  "name": "canopy-charts",
  "version": "1.0.0",
  "description": "Beautiful D3 charts for modern web apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "canopy-charts": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "d3",
    "charts",
    "react",
    "next.js",
    "remix",
    "astro",
    "expo",
    "electron"
