  "Docs": {
    "title": "Documentation",
    "search": "Search documentation...",
    "introduction": {
      "title": "Introduction",
      "welcome": "Welcome to Canopy Charts documentation. This guide will help you get started with our powerful charting library that combines the flexibility of D3.js with the modern features of React.",
      "features": {
        "title": "Key Features",
        "ownership": {
          "title": "Your Code, Your Control",
          "description": "Copy and paste what you need, then customize freely without dependency constraints."
        },
        "modern": {
          "title": "Modern Stack",
          "description": "Built with React + D3.js + Tailwind CSS for beautiful, responsive visualizations."
        },
        "flexible": {
          "title": "Flexible Design",
          "description": "Start with sensible defaults, then customize the implementation and design separately."
        },
        "responsive": {
          "title": "Responsive by Default",
          "description": "Charts automatically adapt to any screen size with smooth animations."
        },
        "typescript": {
          "title": "TypeScript Ready",
          "description": "Full TypeScript support with well-documented props and utilities."
        }
      },
      "philosophy": {
        "title": "Philosophy",
        "description": "Canopy Charts is not a traditional component library. Instead of installing a dependency, you copy and paste the components you need. This gives you complete control over your charts.",
        "why": {
          "title": "Why this approach?",
          "description": "Traditional chart libraries often couple style with implementation. By providing copy-paste components:",
          "benefits": {
            "control": "Full control over implementation and design",
            "dependencies": "No version conflicts or dependency management",
            "learning": "Learn and modify the code as you use it",
            "bundle": "Only include what you need, reducing bundle size",
            "future": "Future-proof: your charts won't break from upstream changes",
            "security": "Better security: review code once, no need to trust external package updates"
          }
        }
      }
    },
